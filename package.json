{
  "name": "markdown-parse-vue",
  "version": "0.2.2",
  "description": "Vue3 Markdown rendering component with TSX support",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@types/marked": "^6.0.0",
    "echarts": "^5.6.0",
    "highlight.js": "^11.11.1",
    "html-to-image": "^1.11.11",
    "markdown-it": "^14.1.0",
    "marked": "^15.0.6",
    "marked-highlight": "^2.2.1",
    "mermaid": "^11.4.1",
    "panzoom": "^9.4.3",
    "remixicon": "^4.6.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^13.0.0",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/runtime-dom": "^3.0.0",
    "@vue/tsconfig": "^0.7.0",
    "sass": "^1.64.2",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vue-tsc": "^2.2.0"
  }
}
